<!-- Start Style Switcher -->
<div class="switcher"></div>
<!-- End Style Switcher -->
<script>
    $(window).load(function () {
        $('.slider').fractionSlider({
            'fullWidth': true,
            'controls': true,
            'responsive': true,
            'dimensions': "1920,450",
            'timeout': 5000,
            'increase': true,
            'pauseOnHover': true,
            'slideEndAnimation': false,
            'autoChange': true
        });
    });
</script>

