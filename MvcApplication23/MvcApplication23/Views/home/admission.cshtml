                    <b>HSC Board :</b><div >
                        @Html.TextBoxFor(Model => Model.hsc_board, new { @class = "form-control" })
                    </div>

                </td>
            </tr>


            <tr>
                <td>
                    <b>HSC TotalMarks:</b><div >
                        @Html.TextBoxFor(Model => Model.hsc_totalmarks, new { @class = "form-control" })
                    </div>
                </td>
                <td>
                    <b>HSC Enrolment:</b><div>
                        @Html.TextBoxFor(Model => Model.hsc_enrolment, new { @class = "form-control" })
                    </div>

                </td>
            </tr>



            <tr>
                <td>
                    <b>Matric Marksheet/Certificate:</b><div >
                                              <input type="file" name="File" id="File" class="form-control" />
                    </div>
                </td>
                <td>
                    <b>Inter Marksheet/Certificate:</b><div >
                 <input type="file" name="File2" id="File2" class="form-control" />

                    </div>

                </td>
            </tr>


            <tr><td><input type="submit" id="submit" value="Add Record!" class="btn btn-success" /></td></tr>




            <tr> <td>@Html.Raw(ViewBag.alertmessage)</td></tr>






        </table>

       

    </div>
}
            
